                price = 5.99
            elif size == "2":
                sandwich = "Footlong Sub"
                price = 8.99
            else:
                print("Invalid choice, defaulting to 6-inch Sub.")
                sandwich = "6-inch Sub"
                price = 5.99
            print("Select Toppings: (Type 'done' when finished)")
            toppings = []
            while True:
                print("Options: Lettuce, Tomato, Onion, Pickles, Olives, Jalapeños, Cucumber")
                topping = input("Add a topping: ").capitalize()
                if topping == "Done":
                    break
                elif topping in ["Lettuce", "Tomato", "Onion", "Pickles", "Olives", "Jalapeños", "Cucumber"]:
                    toppings.append(topping)
                else:
                    print("Invalid topping, please try again.")
            print("Select Dressing:")
            print("1. Mayo")
            print("2. Mustard")
            print("3. Ranch")
            print("4. Chipotle Southwest")
            print("5. No Dressing")
            dressing = input("Select Dressing: ")
            if dressing == "1":
                dressing = "Mayo"
            elif dressing == "2":
                dressing = "Mustard"
            elif dressing == "3":
                dressing = "Ranch"
            elif dressing == "4":
                dressing = "Chipotle Southwest"
            else:
                dressing = "No Dressing"
            custom_sandwich = f"{sandwich} with {', '.join(toppings)} and {dressing}"
            l = Cart()
            l.add_cart(custom_sandwich, price)
        elif option == "0":
            print("Returning to main menu.")
    elif restaurant == "10":
