import tkinter as tk
from tkinter import PhotoImage
import random

window = tk.Tk()

class User:
    def __init__(self):
        self.user_name = ""
        self.user_age = ""
        self.user_address = ""

    def create_user_profile(self):
        self.user_name = input("Enter your name: ")
        self.user_age = input("Enter your age: ")
        self.user_address = input("Enter your address: ")

    def display_user_profile(self):
        print(self.user_name)
        print(self.user_age)
        print(self.user_address)

    def update_user_profile(self):
        while 1:
            print("1. Update your name: ")
            print("2. Update your age: ")
            print("3. Update your address: ")
            x = input()
            if x == "1":
                print("Your current name is ", self.user_name)
                self.user_name = input("What would you like to be called instead? ")
                break
            elif x == "2":
                print("Your current age is ", self.user_age)
                self.user_age = input("What would you like to change it to? ")
                break
            elif x == "3":
                print("Your current address is ", self.user_address)
                self.user_address = input("What is your updated address? ")
                break


class Cart:
    def __init__(self):
        self.items = []

    def add_cart(self, item_name, item_price):
        self.items.append(item_name)
        orderList.append(item_name)
        print(f"{item_name} added to the cart at ${item_price}")
        priceList.append(item_price)

    def remove_cart(self):
        r = input("Which item would you like to remove?: ")
        i = 0
        while r not in orderList:
            print(r, "is not in the cart.")
            r = input("Which item would you like to remove?: ")
        if r in orderList:
            for i in range(0, len(orderList)):
                if orderList[i] == r:
                    del priceList[i]
            orderList.remove(r)
            print(r, "has been removed")

    def display_cart(self):
        x = 0
        for x in range(len(orderList)):
            print(orderList[x], priceList[x])
            x += 1
        total = sum(priceList)
        print("Total =", total)


class Checkout:
    def __init__(self):
        self.pay_type = ""
        self.card_number = ""
        self.cvv = ""
        self.expire = ""
        self.numItems = ""
        self.deliver_time = ""
        self.deliver_person = ""
        self.verse = ""

    def pay(self):
        print("1. Debit")
        print("2. Credit")
        option = input("What are you paying with?: ")
        if option == "1":
            self.pay_type = "Debit"
            self.card_number = input("Enter your debit card number: ")
            self.cvv = input("Enter the security code: ")
            self.expire = input("Enter the expiration date: ")
        elif option == "2":
            self.pay_type = "Credit"
            self.card_number = input("Enter your credit card number: ")
            self.cvv = input("Enter the security code: ")
            self.expire = input("Enter the expiration date: ")

    def delivery(self):
        self.numItems = len(orderList)
        self.deliver_time = 10 + (self.numItems * 5)
        x = random.randint(1, 5)
        if x == 1:
            self.deliver_person = "Henry"
        if x == 2:
            self.deliver_person = "Ronald"
        if x == 3:
            self.deliver_person = "Lily"
        if x == 4:
            self.deliver_person = "Sally"
        if x == 5:
            self.deliver_person = "Luke"
        print(self.deliver_person, "will arrive in", self.deliver_time, "minutes")

    def random_verse(self):
        x = random.randint(1,10)
        if x == 1:
            self.verse = ("Isaiah 41:10: So do not fear, for I am with you; do not be dismayed, "
                          "for I am your God. I will strengthen you and help you; I will uphold "
                          "you with my righteous right hand.")
        if x == 2:
            self.verse = ("Philippians 4:6-7: Do not be anxious about anything, but in every situation, "
                          "by prayer and petition, with thanksgiving, present your requests to God. "
                          "And the peace of God, which transcends all understanding, will guard your "
                          "hearts and your minds in Christ Jesus.")
        if x == 3:
            self.verse = ("Romans 8:28: And we know that in all things God works for the good of those "
                          "who love him, who have been called according to his purpose.")
        if x == 4:
            self.verse = ("Joshua 1:9: Have I not commanded you? Be strong and courageous. Do not be frightened, "
                          "and do not be dismayed, for the Lord your God is with you wherever you go.")
        if x == 5:
            self.verse = ("Matthew 6:31-34: So do not worry, saying, ‘What shall we eat?’ or ‘What shall we drink?’ "
                          "or ‘What shall we wear?’ For the pagans run after all these things, and your heavenly "
                          "Father knows that you need them. But seek first his kingdom and his righteousness, "
                          "and all these things will be given to you as well. Therefore do not worry about tomorrow, "
                          "for tomorrow will worry about itself. Each day has enough trouble of its own.")
        if x == 6:
            self.verse = "Philippians 4:13: I can do all things through him who strengthens me."
        if x == 7:
            self.verse = ("Psalm 28:7: The Lord is my strength and my shield; in him my heart trusts, "
                          "and I am helped; my heart exults, and with my song I give thanks to him.")
        if x == 8:
            self.verse = "Matthew 11:28: Come to me, all who labor and are heavy laden, and I will give you rest."
        if x == 9:
            self.verse = ("Isaiah 43:2: When you pass through the waters, I will be with you; and through the rivers, "
                          "they shall not overwhelm you; when you walk through fire you shall not be burned, "
                          "and the flame shall not consume you.")
        if x == 10:
            self.verse = ("Romans 15:13: May the God of hope fill you with all joy and peace in believing, "
                          "so that by the power of the Holy Spirit you may abound in hope.")

        frame = tk.Frame(master=window, height=100000, width=100000, bg="light blue")
        frame.pack()

        thank = tk.Label(master=window, text="Thank you for shopping with NomNomNow!", font=("Times New Roman",25), fg="black", bg="light blue")
        thank.place(x=450, y=150)

        v = tk.Label(master=window, text=self.verse, font=("Times New Roman",14), fg="black", bg="light blue")
        v.place(x=200, y=400)


